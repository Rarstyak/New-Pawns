          <parts>
            <li>
              <def>InsectHead</def>
              <coverage>0.65</coverage>
              <parts>
                <li>
                  <def>Brain</def>
                  <coverage>0.2</coverage>
                  <depth>Inside</depth>
                </li>
                <li>
                  <def>InsectMouth</def>
                  <coverage>0.05</coverage>
                  <groups>
                    <li>Mouth</li>
                  </groups>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <!--li> example code
          <def>SlimeTentacle</def>
          <coverage>0.06</coverage>
        </li-->
      </parts>
