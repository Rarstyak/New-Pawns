<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <BodyDef>
    <defName>Slimelike</defName>
    <label>slime-like</label>
    <corePart>
      <def>Membrane</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <parts>
		<li>
			<def>Antenna</def>
			<coverage>0.05</coverage>
		</li>
        <li>
          <def>Stomach</def>
          <coverage>0.05</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>SlimeCore</def>
          <coverage>0.03</coverage>
          <depth>Inside</depth>
        </li>
          <def>Pronotum</def>
          <coverage>0.08</coverage>
          <height>Top</height>
          <groups>
            <li>CoveredByNaturalArmor</li>
